  { start = "00:03:33.85", ["end"] = "00:03:36.75", text = "Yes, if I felt the boiling blood" },
  { start = "00:03:39.74", ["end"] = "00:03:42.12", text = "I will stake my fate with you" },
  { start = "00:03:47.62", ["end"] = "00:03:53.11", text = "They are waiting to be free, I lit lights in my eyes" }
